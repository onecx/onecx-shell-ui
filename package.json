{
  "name": "onecx-shell-ui",
  "private": true,
  "version": "0.0.0",
  "license": "Apache-2.0",
  "contributors": [
    "OneCX Development Team <onecx-development-team@onecx.org>"
  ],
  "config": {
    "openapiYaml": "src/assets/openapi/openapi-bff.yaml",
    "openapiOutput": "src/app/shared/generated"
  },
  "scripts": {
    "build": "nx build",
    "clean": "npm cache clean --force && npm run clean-angular",
    "clean-angular": "ng cache clean",
    "start": "nx serve --configuration=development --proxy-config=proxy.conf.js",
    "lint": "nx lint",
    "format": "ng lint --fix",
    "test": "nx test",
    "test:ci": "nx test --coverage --testResultsProcessor=jest-sonar-reporter",
    "apigen-cleanup": "rm -rf $npm_package_config_openapiOutput",
    "apigen-format": "npx prettier $npm_package_config_openapiYaml --write && npx prettier $npm_package_config_openapiOutput/**/* --write",
    "apigen-generate": "openapi-generator-cli generate -i $npm_package_config_openapiYaml -g typescript-angular -c apigen.yaml -o $npm_package_config_openapiOutput --type-mappings=AnyType=object,set=Array",
    "apigen": "npm run apigen-cleanup && npm run apigen-generate && npm run apigen-format"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^15.0.0",
    "@angular/animations": "~15.2.7",
    "@angular/common": "~15.2.7",
    "@angular/compiler": "~15.2.7",
    "@angular/core": "~15.2.7",
    "@angular/forms": "~15.2.7",
    "@angular/platform-browser": "~15.2.7",
    "@angular/platform-browser-dynamic": "~15.2.7",
    "@angular/router": "~15.2.7",
    "@ngx-translate/core": "^14.0.0",
    "@nx/angular": "17.3.1",
    "@onecx/accelerator": "^4.25.0",
    "@onecx/angular-accelerator": "^4.25.0",
    "@onecx/angular-integration-interface": "^4.25.0",
    "@onecx/angular-remote-components": "^4.25.0",
    "@onecx/integration-interface": "^4.25.0",
    "@onecx/keycloak-auth": "^4.25.0",
    "@onecx/portal-integration-angular": "^4.25.0",
    "@onecx/portal-layout-styles": "^4.25.0",
    "@onecx/shell-core": "^4.25.0",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "keycloak-angular": "^13.1.0",
    "keycloak-js": "^18.0.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "15.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.6",
    "@angular-devkit/core": "~15.2.6",
    "@angular-devkit/schematics": "~15.2.6",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cdk": "~15.2.7",
    "@angular/cli": "~17.1.0",
    "@angular/compiler-cli": "~15.2.7",
    "@angular/language-service": "~17.1.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@nx/eslint": "17.3.1",
    "@nx/eslint-plugin": "17.3.1",
    "@nx/jest": "17.3.1",
    "@nx/js": "17.3.1",
    "@nx/workspace": "17.3.1",
    "@schematics/angular": "~15.2.7",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.4.13",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.4",
    "jest-sonar-reporter": "^2.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "17.3.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "nx": {
    "includedScripts": []
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "sonarqube_report.xml"
  }
}
